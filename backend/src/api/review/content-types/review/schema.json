{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "rating": {
      "type": "integer"
    },
    "publishedDate": {
      "type": "date"
    },
    "cover": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "genre": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "category"
    },
    "featured": {
      "type": "boolean"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "pelicula",
        "serie",
        "videojuego"
      ]
    },
    "seo": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    }
  }
}
